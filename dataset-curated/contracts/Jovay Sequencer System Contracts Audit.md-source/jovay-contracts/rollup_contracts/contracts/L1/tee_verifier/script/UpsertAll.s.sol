    function bytes32ToHex(bytes32 b) public pure returns (string memory) {
        bytes memory result = new bytes(64);

        for (uint256 i = 0; i < 32; i++) {
            uint8 byteValue = uint8(b[i]);
            result[i * 2] = byteToHexChar(byteValue / 16);
            result[i * 2 + 1] = byteToHexChar(byteValue % 16);
        }

        return string(result);
    }

    function byteToHexChar(uint8 b) internal pure returns (bytes1) {
        if (b < 10) {
            return bytes1(b + 0x30); // '0' to '9'
        } else {
            return bytes1(b + 0x57); // 'a' to 'f'
        }
    }

    function copyBytesToBytes32(bytes memory original, uint256 start) public pure returns (bytes32) {
        require(original.length >= start + 32, "Not enough data to read");

        bytes32 result;

        // copy data to bytes32
        assembly {
            result := mload(add(original, add(start, 0x20))) // +0x20 represent length
        }

        return result;
    }

