    error InvalidHexLength();
    error InvalidCharacter(uint8 c);

    function hexStringToBytes(string memory s) public pure returns (bytes memory) {
        bytes memory b = bytes(s);
        uint256 len = b.length;
        uint256 offset = 0;

        if (len >= 2 && b[0] == "0" && (b[1] == "x" || b[1] == "X")) {
            offset = 2;
        }

        if ((len - offset) % 2 != 0) {
            revert InvalidHexLength();
        }

        uint256 outLen = (len - offset) / 2;
        bytes memory out = new bytes(outLen);

        for (uint256 i = 0; i < outLen; i++) {
            uint256 pos = offset + i * 2;

            uint8 h = charToByte(uint8(b[pos]));
            uint8 l = charToByte(uint8(b[pos + 1]));

            out[i] = bytes1((h << 4) | l);
        }

        return out;
    }

    function charToByte(uint8 c) private pure returns (uint8) {
        if (c >= 48 && c <= 57) {
            return c - 48; // '0' - '9'
        } else if (c >= 65 && c <= 70) {
            return c - 55; // 'A' - 'F'
        } else if (c >= 97 && c <= 102) {
            return c - 87; // 'a' - 'f'
        }
        revert InvalidCharacter(c);
    }
