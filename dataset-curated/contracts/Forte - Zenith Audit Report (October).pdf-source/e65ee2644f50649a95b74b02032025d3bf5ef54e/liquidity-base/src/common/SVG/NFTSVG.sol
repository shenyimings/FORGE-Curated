                "</text>",
                '<text font-family="Helvetica, Arial, sans-serif" font-size="14" fill="#F8F8F8" letter-spacing="0.5px">',
                '<textPath href="#borderPath" startOffset="0%">',
                '<animate attributeName="startOffset" ',
                'from="0%" to="100%" ',
